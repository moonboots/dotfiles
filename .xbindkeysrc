# vol up
"/usr/bin/pactl set-sink-volume 0 -- +5%"
  m:0x0 + c:123
  
# vol dn
"/usr/bin/pactl set-sink-volume 0 -- -5%"
  m:0x0 + c:122

# vol mute/unmute
"/usr/bin/pactl set-sink-mute 0 toggle"
  m:0x0 + c:121

